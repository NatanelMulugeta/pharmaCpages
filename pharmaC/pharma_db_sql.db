
    create table pharmac_branch (
       branch_no bigint not null auto_increment,
        branch_name varchar(255),
        kebele varchar(255),
        official_phone_no varchar(255),
        sub_city varchar(255),
        primary key (branch_no)
    ) engine=InnoDB

    create table pharmacy (
       pharmacy_license_id bigint not null auto_increment,
        official_email varchar(255),
        password varchar(255),
        pharmacy_name varchar(255),
        re_password varchar(255),
        primary key (pharmacy_license_id)
    ) engine=InnoDB

    create table pharmacy_pharmac_locations (
       pharmacy_pharmacy_license_id bigint not null,
        pharmac_locations_branch_no bigint not null,
        primary key (pharmacy_pharmacy_license_id, pharmac_locations_branch_no)
    ) engine=InnoDB

    alter table pharmacy_pharmac_locations 
       add constraint FK5hcoi4ftddp31t0ujjnf90nrs 
       foreign key (pharmac_locations_branch_no) 
       references pharmac_branch (branch_no)

    alter table pharmacy_pharmac_locations 
       add constraint FKgaoxtyr59i5t0avxeay9rydmc 
       foreign key (pharmacy_pharmacy_license_id) 
